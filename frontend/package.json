		"@types/react-dom": "^18.0.10",
		"@vitejs/plugin-legacy": "^6.0.0",
		"@vitejs/plugin-react": "^4.3.4",
		"eslint": "^8.35.0",
		"eslint-plugin-react": "^7.37.4",
		"jsdom": "^26.0.0",
		"prettier": "^3.4.2",
		"react-barcode-scanner": "^3.0.1",
		"typescript": "^5.7.3",
		"vite": "^6.2.2",
