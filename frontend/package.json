{
	"name": "localib",
	"description": "Collects your Discogs library for offline usage",
	"private": true,
	"version": "develop",
	"type": "module",
	"scripts": {
		"start": "npm run serve",
		"dev": "vite",
		"build": "tsc && vite build",
		"build:ion": "tsc && ionic build --prod",
		"serve": "ionic serve --host=0.0.0.0",
		"preview": "vite preview",
		"test.unit": "vitest",
		"lint": "eslint",
		"format": "prettier . --write"
	},
	"dependencies": {
		"@capacitor/app": "^8.0.0",
		"@capacitor/core": "^8.0.2",
		"@capacitor/haptics": "^8.0.0",
		"@capacitor/keyboard": "^8.0.0",
		"@capacitor/status-bar": "^8.0.0",
		"@ionic/react": "^8.7.16",
		"@ionic/react-router": "^8.7.16",
		"@tanstack/query-sync-storage-persister": "^5.90.22",
		"@tanstack/react-query": "^5.90.20",
		"@tanstack/react-query-persist-client": "^5.90.22",
		"@types/react-router": "^5.1.20",
		"@types/react-router-dom": "^5.3.3",
		"idb-keyval": "^6.2.2",
		"ionicons": "^8.0.8",
		"qrcode.react": "^4.2.0",
		"react": "^18.2.0",
