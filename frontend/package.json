{
	"name": "localib",
	"description": "Collects your Discogs library for offline usage",
	"private": true,
	"version": "develop",
	"type": "module",
	"scripts": {
		"start": "npm run serve",
		"dev": "vite",
		"build": "tsc && vite build",
		"build:ion": "tsc && ionic build --prod",
		"serve": "ionic serve --host=0.0.0.0",
		"preview": "vite preview",
		"test.unit": "vitest",
		"lint": "eslint",
		"format": "prettier . --write"
	},
	"dependencies": {
		"@capacitor/app": "^7.0.0",
		"@capacitor/core": "^7.0.1",
		"@capacitor/haptics": "^7.0.0",
		"@capacitor/keyboard": "^7.0.0",
		"@capacitor/status-bar": "^7.0.0",
		"@ionic/react": "^8.4.3",
		"@ionic/react-router": "^8.4.3",
		"@tanstack/query-sync-storage-persister": "^5.66.0",
		"@tanstack/react-query": "^5.66.0",
		"@tanstack/react-query-persist-client": "^5.66.0",
		"@types/react-router": "^5.1.20",
		"@types/react-router-dom": "^5.3.3",
		"idb-keyval": "^6.2.1",
		"ionicons": "^7.0.0",
		"qrcode.react": "^4.2.0",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-router": "^5.3.4",
		"react-router-dom": "^5.3.4"
