		"@vitejs/plugin-legacy": "^7.2.1",
		"@vitejs/plugin-react": "^5.1.2",
		"eslint": "^8.35.0",
		"eslint-plugin-react": "^7.37.5",
		"jsdom": "^27.4.0",
		"prettier": "^3.8.1",
		"react-barcode-scanner": "^4.0.0",
		"typescript": "^5.9.3",
		"vite": "^7.3.0",
